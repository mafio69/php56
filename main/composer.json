{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	
	"require": {
      "laravel/framework": "4.2.*",
      "laravelbook/ardent": "2.5.0",
      "barryvdh/laravel-debugbar": "1.*",
      "barryvdh/laravel-ide-helper": "1.*",
      "intervention/image": "1.*",
      "barryvdh/laravel-dompdf": "0.4.*",
      "phpoffice/phpexcel": "1.8.0",
      "maatwebsite/excel": "~1.2.1",
      "way/generators": "2.*",
      "laracasts/flash": "~1.0",
      "parsecsv/php-parsecsv": "0.4.*",
      "jenssegers/date": "^3.2",
      "pda/pheanstalk": "~2.0",
      "tedivm/fetch": "^0.7.1",
      "iio/libmergepdf": "^3.1",
      "doctrine/dbal": "2.5.*",
		"tymon/jwt-auth": "0.4.*",
		"guzzlehttp/guzzle": "~6.0",
		"setasign/fpdf": "^1.8",
		"phpmailer/phpmailer": "~5.2",
		"tomgrohl/laravel4-php71-encrypter": "^1.1",
		"willdurand/email-reply-parser": "^2.9",
		"vaibhavpandeyvpz/phemail": "^1.0",
		"php-mime-mail-parser/php-mime-mail-parser": "3.0.4",
		"zbateson/mail-mime-parser": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
            "Idea\\": "app/Idea"
        },
		"files": ["app/includes/helpers.php"]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
